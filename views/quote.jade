doctype html
html
	head
		title Quoteboard
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		style.
			img {
				width: 33%;
				position:absolute;
				top: 0;
				bottom: 0;
				left:  0;
				right: 0;
				margin: auto;
			}
	body
		span.helper
		img(src=url)
		script.
			setTimeout(function () {
				window.location = '/quote';
			}, 10000);